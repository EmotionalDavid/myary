<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shi.home.mapper.BoardMapper">
<select id="boardTest" resultType="String">
select sysdate from dual
</select>
<insert id="insertTest">
insert into test  values ('test'|| to_char(sysdate,'MMDD HH:mi:SS'))
</insert>

</mapper>